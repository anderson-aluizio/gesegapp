{
  "expo": {
    "name": "Geseg",
    "slug": "gesegapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dinamo.gesegapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dinamo.gesegapp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/splash.png",
          "imageWidth": 300,
          "backgroundColor": "#ffffff",
          "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.dinamo.gesegapp"
          },
          "android": {
            "adaptiveIcon": {
              "foregroundImage": "./assets/img/adaptive-icon.png",
              "backgroundColor": "#ffffff"
            },
            "package": "com.dinamo.gesegapp"
          }
        }
      ],
      "expo-location",
      "expo-font",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56bea8cc-f60f-4b61-95a7-c59d86d42890"
      }
    }
  }
}
