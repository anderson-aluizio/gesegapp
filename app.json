{
  "expo": {
    "name": "Geseg",
    "slug": "geseg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dinamo.geseg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dinamo.geseg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/splash.png",
          "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.dinamo.geseg"
          },
          "android": {
            "adaptiveIcon": {
              "foregroundImage": "./assets/img/adaptive-icon.png",
              "backgroundColor": "#ffffff"
            },
            "package": "com.dinamo.geseg"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "gesegapp",
          "organization": "dinamo-engenharia"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "410d37af-c4a6-4b0e-a17d-1aa4448400ff"
      }
    }
  }
}
